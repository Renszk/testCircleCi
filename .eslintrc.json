{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "vue",
    "plugin:vue/recommended",
    "eslint:recommended",
    "plugin:vue/base",
    "plugin:vue/essential",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "globals": {
    "Vue": true
  },
  "rules": {
    "no-console": "warn",
    "no-var": "error",
    "prefer-const": "error",
    "indent": ["error", 2]
  }
}